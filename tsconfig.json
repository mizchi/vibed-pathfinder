{
  "compilerOptions": {
    "target": "es2024",
    "module": "esnext",
    "noEmit": true,
    "moduleResolution": "bundler",
    "strict": true,
    "importsNotUsedAsValues": "error",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true
  }
}
